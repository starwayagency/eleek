doskey manage=python manage.py 
doskey r=python manage.py runserver 
doskey m=python manage.py migrate 
doskey mm=python manage.py makemigrations 
doskey csu=python manage.py createsuperuser 
doskey load=python manage.py loaddata 
doskey makem=python manage.py makemessages 
doskey makemj=python manage.py makemessages -d djangojs 
doskey compile=python manage.py compilemessages 
doskey req=pip install -r requirements.txt 
doskey freeze=pip freeze > requirements.txt
doskey sv=.\w_venv\Scripts\activate
doskey dv=deactivate
doskey ls=dir

https://stackoverflow.com/questions/42606837/how-do-i-use-bash-on-windows-from-the-visual-studio-code-integrated-terminal



echo alias manage="python manage.py" >> ~/.bashrc
echo alias r="python manage.py runserver" >> ~/.bashrc
echo alias m="python manage.py migrate" >> ~/.bashrc
echo alias mm="python manage.py makemigrations" >> ~/.bashrc
echo alias csu="python manage.py createsuperuser" >> ~/.bashrc
echo alias load="python manage.py loaddata" >> ~/.bashrc
echo alias makem="python manage.py makemessages" >> ~/.bashrc
echo alias makemj="python manage.py makemessages -d djangojs" >> ~/.bashrc 
echo alias compile="python manage.py compilemessages" >> ~/.bashrc
echo alias req="pip install -r requirements.txt" >> ~/.bashrc
echo alias freeze="pip freeze > requirements.txt" >> ~/.bashrc
echo alias sv="source venv/bin/activate" >> ~/.bashrc
echo alias dv="deactivate" >> ~/.bashrc



