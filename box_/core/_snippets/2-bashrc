alias bashrc=". ~/.bashrc"
alias pip="pip3"
alias python="python3"
alias sv="source venv/bin/activate"
alias dv="deactivate"
alias venv="python -m venv venv"
alias req="pip install -r requirements.txt"
alias freeze="pip freeze | grep -v \"pkg-resources\" > requirements.txt"
alias m="python manage.py migrate"
alias mm="python manage.py makemigrations"
alias r="python manage.py runserver 127.0.0.1:8000"
alias r0="python manage.py runserver 0.0.0.0:8000"
alias csu="python manage.py createsuperuser"
alias load="python manage.py loaddata"
alias gac="git add . && git commit -m"
alias makem="python manage.py makemessages"
alias makemjs="python manage.py makemessages -d djangojs"
alias compile="python manage.py compilemessages"
alias collect="python manage.py collectstatic"
alias remote="git remote add origin"
alias remove="git remote remove"
alias push="git push origin master"
alias pull="git pull origin master" 
alias rmmig="find . -path \"*/migrations/*.py\" -not -name \"__init__.py\" -delete"

